<div class="sdg-graph" id="<%= idName %>">hello everyone!</div>
<style>
    .sdg-graph{
        height: 500px;
        width: 500px;
    }
</style>
<script src="https://cdnjs.cloudflare.com/ajax/libs/plotly.js/1.33.1/plotly-basic.min.js"></script>
<script>
    function plotter(data){
        console.log("what did?",data);
        graphMaker(data,"<%= idName %>");
    }
    (function grapher() {
        $.ajax({
            url: '/simple-data-grapher/fetch_graphobject',
            type: 'get',
            data: {uid: <%= uid %>, id: <%= id %>},
            success: function(data){
                console.log("data yassss",data);
                plotter(data);
            },
            error: function(XMLHttpRequest, textStatus, errorThrown){
                console.log("error");
            }
        });
    })()
</script>